		"action": ACTION,
		"target": TARGET,
		"pre": [TEST],
		"post": POST
	},
	"2": {
		"type": RAP (1),
		"action": [id],
		"post": POST,
		"pre": [TEST],
		"goal": GOALS,
		"priority": 0 < i <= 1
	}
}

